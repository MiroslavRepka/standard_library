data Field {
	Type type
	char name[]
const int FLAG_RECURSION = 0x1
	byte flags
	}

data Type {
const byte INTEGER		= 1
const byte DECIMAL		= 2
const byte DATA			= 3
const byte OBJECT		= 4
const byte ARRAY		= 5
	byte class

const byte F_BOOL		= 0x1
const byte F_CHAR		= 0x2
const byte F_FUNCTION	= 0x4
const byte F_EVENT		= 0x8
	byte flags

	int size
	Field fields[]
	}